            "minecraft:on_calm": { "remove": { "component_groups": [ "minecraft:silverfish_angry" ] }, "add": { "component_groups": [ "minecraft:silverfish_calm" ] } },
            
            "team_red": { "add": { "component_groups": [ "team_red", "bedwars_silverfish" ] } },
