                "14_health": { "on_entry": [ "/scoreboard players set @s health 14" ], "transitions": [ { "default": "1" } ] },
                "15_health": { "on_entry": [ "/scoreboard players set @s health 15" ], "transitions": [ { "default": "1" } ] },
                "16_health": { "on_entry": [ "/scoreboard players set @s health 16" ], "transitions": [ { "default": "1" } ] },
                "17_health": { "on_entry": [ "/scoreboard players set @s health 17" ], "transitions": [ { "default": "1" } ] },
                "18_health": { "on_entry": [ "/scoreboard players set @s health 18" ], "transitions": [ { "default": "1" } ] },
                "19_health": { "on_entry": [ "/scoreboard players set @s health 19" ], "transitions": [ { "default": "1" } ] },
                "20_health": { "on_entry": [ "/scoreboard players set @s health 20" ], "transitions": [ { "default": "1" } ] },
                "21_health": { "on_entry": [ "/scoreboard players set @s health 21" ], "transitions": [ { "default": "1" } ] },
                "22_health": { "on_entry": [ "/scoreboard players set @s health 22" ], "transitions": [ { "default": "1" } ] },
                "23_health": { "on_entry": [ "/scoreboard players set @s health 23" ], "transitions": [ { "default": "1" } ] },
                "24_health": { "on_entry": [ "/scoreboard players set @s health 24" ], "transitions": [ { "default": "1" } ] },
