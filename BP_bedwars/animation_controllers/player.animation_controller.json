                "26_health": { "on_entry": [ "/scoreboard players set @s health 26" ], "transitions": [ { "default": "1" } ] },
                "27_health": { "on_entry": [ "/scoreboard players set @s health 27" ], "transitions": [ { "default": "1" } ] },
                "28_health": { "on_entry": [ "/scoreboard players set @s health 28" ], "transitions": [ { "default": "1" } ] },
                "29_health": { "on_entry": [ "/scoreboard players set @s health 29" ], "transitions": [ { "default": "1" } ] },
                "30_health": { "on_entry": [ "/scoreboard players set @s health 30" ], "transitions": [ { "default": "1" } ] }
            }
        }
    }
