{
    "format_version": "1.10.0",
    "animation_controllers": {
        "controller.animation.player.drinking_magic_milk": {
            "states": {
                "default": {
                    "transitions": [
                        { "drinking_magic_milk": "query.is_item_name_any('slot.weapon.mainhand', 0, 'bedwars:magic_milk') && query.is_using_item" }
                    ]
                },
                "drinking_magic_milk": {
                    "animations": [ "drinking_magic_milk" ],
                    "transitions": [
                        { "default": "!query.is_item_name_any('slot.weapon.mainhand', 0, 'bedwars:magic_milk') || !query.is_using_item" }
                    ]
                }
            }
        },
        "controller.animation.player.is_holding_dream_defender_test": {
            "states": {
                "default": {
                    "animations": [ "is_holding_dream_defender_test" ],
                    "transitions": [
                        { "is_holding_dream_defender": "query.is_item_name_any('slot.weapon.mainhand', 0, 'bedwars:dream_defender')" }
                    ]
                },
                "is_holding_dream_defender": {
                    "on_entry": [ "/tag @s add isHoldingDreamDefender" ],
                    "transitions": [
                        { "default": "!query.is_item_name_any('slot.weapon.mainhand', 0, 'bedwars:dream_defender')" }
                    ]
                }
            }
        },
        "controller.animation.player.is_invisibility_test": {
            "states": {
                "default": {
                    "on_entry": [ "@s show_armor" ],
                    "transitions": [
                        { "is_invisibility": "query.is_invisible && query.has_any_family('have_team')" }
                    ]
                },
                "is_invisibility": {
                    "on_entry": [ "@s hide_armor" ],
                    "transitions": [
                        { "default": "!query.is_invisible || !query.has_any_family('have_team')" }
                    ]
                }
            }
        },

        "controller.animation.player.last_hit_by_player_test": {
            "states": {
                "default": {
                    "on_entry": [ "/tag @s remove lastHitByPlayer" ],
                    "transitions": [ { "last_hit_by_player": "query.last_hit_by_player" } ]
                },
                "last_hit_by_player": {
                    "on_entry": [ "/tag @s add lastHitByPlayer" ],
                    "transitions": [ { "default": "!query.last_hit_by_player" } ]
                }
            }
        },

        "controller.animation.player.damage_type_test": {
            "states": {
                "default": {
                    "transitions": [
                        { "attack": "query.mark_variant == 1" },
                        { "shoot": "query.mark_variant == 2" },
                        { "cause_fall": "query.mark_variant == 3" },
                        { "kill": "query.mark_variant == 4" }
                    ]
                },
                "attack": {
                    "animations": [ "attack_test" ],
                    "transitions": [
                        { "default": "query.mark_variant != 1" }
                    ]
                },
                "shoot": {
                    "animations": [ "shoot_test" ],
                    "transitions": [
                        { "default": "query.mark_variant != 2" }
                    ]
                },
                "cause_fall": {
                    "animations": [ "cause_fall_test" ],
                    "transitions": [
                        { "default": "query.mark_variant != 3" }
                    ]
                },
                "kill": {
                    "animations": [ "kill_test" ],
                    "transitions": [
                        { "default": "query.mark_variant != 4" }
                    ]
                }
            }
        },

        "controller.animation.player.health_test": {
            "states": {
                "default": {
                    "transitions": [
                        { "0_health": "query.health == 0" },
                        { "1_health": "query.health >= 0 && query.health <= 1" },
                        { "2_health": "query.health >= 1 && query.health <= 2" },
                        { "3_health": "query.health >= 2 && query.health <= 3" },
                        { "4_health": "query.health >= 3 && query.health <= 4" },
                        { "5_health": "query.health >= 4 && query.health <= 5" },
                        { "6_health": "query.health >= 5 && query.health <= 6" },
                        { "7_health": "query.health >= 6 && query.health <= 7" },
                        { "8_health": "query.health >= 7 && query.health <= 8" },
                        { "9_health": "query.health >= 8 && query.health <= 9" },
                        { "10_health": "query.health >= 9 && query.health <= 10" },
                        { "11_health": "query.health >= 10 && query.health <= 11" },
                        { "12_health": "query.health >= 11 && query.health <= 12" },
                        { "13_health": "query.health >= 12 && query.health <= 13" },
                        { "14_health": "query.health >= 13 && query.health <= 14" },
                        { "15_health": "query.health >= 14 && query.health <= 15" },
                        { "16_health": "query.health >= 15 && query.health <= 16" },
                        { "17_health": "query.health >= 16 && query.health <= 17" },
                        { "18_health": "query.health >= 17 && query.health <= 18" },
                        { "19_health": "query.health >= 18 && query.health <= 19" },
                        { "20_health": "query.health >= 19 && query.health <= 20" },
                        { "21_health": "query.health >= 20 && query.health <= 21" },
                        { "22_health": "query.health >= 21 && query.health <= 22" },
                        { "23_health": "query.health >= 22 && query.health <= 23" },
                        { "24_health": "query.health >= 23 && query.health <= 24" },
                        { "25_health": "query.health >= 24 && query.health <= 25" },
                        { "26_health": "query.health >= 25 && query.health <= 26" },
                        { "27_health": "query.health >= 26 && query.health <= 27" },
                        { "28_health": "query.health >= 27 && query.health <= 28" },
                        { "29_health": "query.health >= 28 && query.health <= 29" },
                        { "30_health": "query.health >= 29 && query.health <= 30" }
                    ]
                },
                "0_health": { "on_entry": [ "/scoreboard players set @s health 0" ], "transitions": [ { "default": "1" } ] },
                "1_health": { "on_entry": [ "/scoreboard players set @s health 1" ], "transitions": [ { "default": "1" } ] },
                "2_health": { "on_entry": [ "/scoreboard players set @s health 2" ], "transitions": [ { "default": "1" } ] },
                "3_health": { "on_entry": [ "/scoreboard players set @s health 3" ], "transitions": [ { "default": "1" } ] },
                "4_health": { "on_entry": [ "/scoreboard players set @s health 4" ], "transitions": [ { "default": "1" } ] },
                "5_health": { "on_entry": [ "/scoreboard players set @s health 5" ], "transitions": [ { "default": "1" } ] },
                "6_health": { "on_entry": [ "/scoreboard players set @s health 6" ], "transitions": [ { "default": "1" } ] },
                "7_health": { "on_entry": [ "/scoreboard players set @s health 7" ], "transitions": [ { "default": "1" } ] },
                "8_health": { "on_entry": [ "/scoreboard players set @s health 8" ], "transitions": [ { "default": "1" } ] },
                "9_health": { "on_entry": [ "/scoreboard players set @s health 9" ], "transitions": [ { "default": "1" } ] },
                "10_health": { "on_entry": [ "/scoreboard players set @s health 10" ], "transitions": [ { "default": "1" } ] },
                "11_health": { "on_entry": [ "/scoreboard players set @s health 11" ], "transitions": [ { "default": "1" } ] },
